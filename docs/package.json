{
  "name": "docs",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vitepress --port 3333 --open",
    "build": "nr prefetch && vitepress build",
    "build-no-prefetch": "vitepress build",
    "serve": "vitepress serve",
    "preview-https": "pnpm run build && serve .vitepress/dist",
    "preview-https-no-prefetch": "pnpm run build-no-prefetch && serve .vitepress/dist",
    "prefetch": "esno .vitepress/scripts/fetch-avatars.ts",
    "generate-pwa-icons": "pwa-assets-generator"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "jiti": "^1.18.2",
    "vue": "latest"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.16",
    "@unocss/reset": "^0.51.8",
    "@vite-pwa/assets-generator": "^0.0.3",
    "@vite-pwa/vitepress": "^0.2.0",
    "@vitejs/plugin-vue": "latest",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "https-localhost": "^4.7.1",
    "unocss": "^0.51.8",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.16.4",
    "vitepress": "1.0.0-beta.2",
    "workbox-window": "^7.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
